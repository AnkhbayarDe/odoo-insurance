<odoo>
    <template id="report_insurance_policy_template">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <div class="page">
                    <h2>Insurance Report</h2>
                    <p>From: <t t-esc="doc.start_date"/> to <t t-esc="doc.end_date"/></p>
                    <p>Policy Type: <t t-esc="doc.policy_type_id.name"/></p>
                </div>
            </t>
        </t>
    </template>
</odoo>