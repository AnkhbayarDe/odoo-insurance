<record id="ir_cron_ai_detection_example" model="ir.cron">
    <field name="name">AI Detection CRON</field>
    <field name="model_id" ref="model_ai_detection"/>
    <field name="state">code</field>
    <field name="code">model.cron_example_action()</field>
    <field name="interval_number">1</field>
    <field name="interval_type">days</field>
    <field name="numbercall">-1</field>
    <field name="nextcall" eval="(DateTime.now() + timedelta(hours=8)).replace(hour=0, minute=0, second=0, microsecond=0).isoformat()" />
    <field name="active">True</field>
</record>
